{
  "name": "dag-builder",
  "version": "0.9.1",
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "prettier:check": "prettier --check '**/*.{html,ts,tsx,js,jsx,css,json}'",
